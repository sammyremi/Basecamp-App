        <h1>COLLABORATE</h1>
        </br>

        <% if signed_in? %>
        <% if current_user.admin?%>
            <p>hi Admin</p>
            <div id="users">
                <% @users.each do |user| %>
                    <p>
                    <strong>Name:</strong>
                    <%= user.name %>
                    </p>

                    <p>
                        <strong>Email:</strong>
                        <%= user.email %>
                    </p>
                    <%= link_to "Edit", edit_user_registration_path %>
                    <%= link_to "Make Admin", new_user_registration_path %>
                    <%= link_to "Delete", cancel_user_registration_path %>
                <% end %>
            </div>
        <% end %>

        <%else%>
            <div>

                <%= link_to "Sign Up", new_user_registration_path, class: "btn btn-primary" %>
                <%= link_to "Login", new_user_session_path, class: "btn btn-primary" %>

            </div>
        <%end%>



