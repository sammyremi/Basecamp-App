
  <p style="color: green"><%= notice %></p>

  <h1>Projects</h1>

  <div id="projects">
    <% @projects.each do |project| %>
      <% if project.user == current_user%>
        <%= render project %>
        <p>
          <%= link_to "Show this project", project %>
        </p>
      <%end%>
    <% end %>
    
  </div>
  <%= link_to "New project", new_project_path %>


